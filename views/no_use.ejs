        slider.style.transform = 'translateX(0)';
        hin.style.display = 'block';
        eng.style.display = "none";
        hi.style.color ="green";
        en.style.color = "#555";
        
        languageSwitch.querySelector('.slider').style.backgroundColor = '#ddd';
